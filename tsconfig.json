{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2019",
    "outDir": "build",
    "rootDir": "src",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "noImplicitAny": true,
  },
  "exclude": [
    "node_modules"
  ]
}
